{
  "constantVariable": {
    "prefix": "cv",
    "body": "const ${1:name} = $0",
    "description": "Constant Variable"
  },
  "constantString": {
    "prefix": "cs",
    "body": "const ${1:name} = '$2'$0",
    "description": "Constant String"
  },
  "constantTemplateLiterals": {
    "prefix": "ctl",
    "body": "const ${1:name} = `$2\\${${3:expression}\\}$4`$0",
    "description": "Constant Template Literals"
  },
  "constantNumber": {
    "prefix": "cn",
    "body": "const ${1:name} = ${0:0}",
    "description": "Constant Number"
  },
  "constantBoolean": {
    "prefix": "cb",
    "body": "const ${1:name} = ${2|true,false|}$0",
    "description": "Constant Boolean"
  },
  "constantObject": {
    "prefix": "co",
    "body": "const ${1:name} = {$2}$0",
    "description": "Constant Object"
  },
  "constantArray": {
    "prefix": "ca",
    "body": "const ${1:name} = [$2]$0",
    "description": "Constant Array"
  },
  "constantPromise": {
    "prefix": "cp",
    "body": [
      "const ${1:name} = new Promise((resolve, reject) => {",
      "\t$0",
      "})"
    ],
    "description": "Constant Promise"
  }
}
